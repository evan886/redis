port 7003
bind 192.168.11.236 127.0.0.1
#enable cluster mode
cluster-enabled yes
appendfilename "appendonly-7003.aof"
cluster-node-timeout 15000
appendonly yes
appendfsync everysec
# redis 这个不能打开 
#daemonize yes
dbfilename dump_7003.rdb
#集群内配置文件
cluster-config-file "nodes-7003.conf"
protected-mode yes
dir /data/redis/

logfile /var/log/redis/7003.log
